# Raven Backend Environment Variables
# Copy this file to .env and fill in your values

# Supabase - Get these from your Supabase project settings > API
# IMPORTANT: Use the SERVICE_ROLE key (not anon key) for the backend
# The service_role key bypasses Row Level Security (RLS)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-service-role-key

# Groq API (Free) - Get from console.groq.com
GROQ_API_KEY=gsk_your-key

# Twilio (WhatsApp) - Get from console.twilio.com
# Use the sandbox number for testing: whatsapp:+14155238886
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# Debug mode (set to false in production)
DEBUG=true
